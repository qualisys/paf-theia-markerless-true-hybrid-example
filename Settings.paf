Project ID: Open PAF Theia Markerless Example
Package Information:
  Name: Theia Markerless Example
  Version: 1.0.1
  Required QTM version: 2020.2.5770
  Encoding bytes: €€€€€

Root type: Subject
Default fields: [Creation date, Creation time]

Date format: Iso
Date separator: Dash
Filename filter: \.(pdf|cmo|cmz|docx)$

Types:
  Subject:
    Participant:
      Fields: [ID, Name, Sex, DOB, Height, Weight]
      Children: [Session]
      Directory pattern: $Name$_$ID$

  Session:
    True Hybrid Session:
      Fields: [Session ID, Test condition, Operator]
      Measurements: [Object, Dynamic]
      Analyses: [Theia and V3D Hybrid Processing]
      Display order: [Session ID, Test condition, Operator, Comments]
      Directory pattern: $Creation date$_Markerless_Hybrid

Measurements:
  Fields:
    Measurement length: 10
    Minimum count: 1
    Maximum count: 1
    Count: 1
  Static:
    Maximum count: 1
    Count: 1
    Measurement type: Static
    Fields: [Theia c3d file]
  Dynamic:
    Maximum count: 5
    Count: 3
    Measurement type: Dynamic
    Fields: [Theia c3d file]
  Object:
    Maximum count: 1
    Count: 1
    Measurement type: Object
    Fields: [Theia c3d file]
    Process With Theia: false

Analyses:
  Theia and V3D Hybrid Processing:
    Type: Compound
    Components: [Internal Run Theia, Theia Tools Process, Visual3D Hybrid]
  Visual3D Hybrid:
    Type: External program
    Program display name: Visual 3d
    Export session: Yes
    Export measurements: c3d
    Arguments: [/s, $WorkingDirectory$create_workspace.v3s]
    Template files: ['*.v3s']
    Do not wait for Application: Yes
  Internal Run Theia:
    Type: External program
    Program display name: Theia
    Do not wait for Application: Yes
    Arguments: [-path, $TemplateDirectory$Scripts\src\Theia\theia_batch_commands.txt, -force-single-instance]
  Theia Tools Process:
    Type: Instantiate template
    Export session: Yes
    Template: Templates\Scripts\src\Theia\run_theia_tools.php
    Export measurements: [c3d,xml settings]

Fields:
  ID:
    Type: String
    Inherit: Connect
  Name:
    Type: String
    Inherit: Connect
    Force: Yes
  DOB:
    Type: Date
    Inherit: Connect
  Creation date:
    Type: Date
    Readonly: Yes
  Creation time:
    Type: TimeOfDay
    Readonly: Yes
  Session ID:
    Type: String
    Inherit: Connect
  Operator:
    Type: String
    Inherit: Copy
  Height:
    Type: Float
    Quantity: Length
    Unit: centimeters
    Decimals: 0
    Inherit: Connect
    Max: 250
    Min: 50
  Weight:
    Type: Float
    Quantity: Mass
    Unit: kilograms
    Decimals: 1
    Inherit: Copy
    Max: 250
    Min: 0
  Sex:
    Type: Enum
    Values: [Unspecified, Female, Male, Other]
    Inherit: Connect
    Default: Unspecified
  Measurement type:
    Type: String
  Test condition:
    Type: String
  Theia c3d file:
    Type: Enum
    Values: [0, 1, 2, 3, 4, 5]
    Default: 0
  Process With Theia:
    Type: Boolean
    Default: true
    Hidden: Yes

Columns:
  Default column definitions:
    ID:
      Width: 200
      Fields:
        Default: Filename
        Participant: ID
    Name:
      Width: 150
      Fields: 
        Participant: Name
    Created:
      Width: 65
      Field: Creation date
    Comments:
      Field: Comments
      Width: 100
